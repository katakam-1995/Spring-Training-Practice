<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context.xsd">

<bean id="department" class="com.otsi.model.Department">
<property name="name" value="Digital Transformation" />
</bean>
<bean id="employee" class="com.otsi.model.Employee" autowire="byName"></bean>

<bean name="employeeServiceByType" class="com.otsi.service.EmployeeByType" autowire="byType" />

<bean name="employeeServiceConstructor" class="com.otsi.service.EmployeeByConstructor" autowire="constructor" />


</beans>